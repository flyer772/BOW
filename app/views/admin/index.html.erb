<% @apps.each do |app|%> <!--start loop-->
<h2>All Applications</h2>
<div class="row">
	<div class="col-sm-8">
		<%= app.contact_email %>
	</div>
	<div class="col-sm-4">
		<%= link_to("View", admin_path(app),:class => 'btn btn-info') %>
		<button type="button" class="btn btn-success">Approve</button>
		<button type="button" class="btn btn-danger">Delete</button>
	</div>
</div>
<%end%>
